{
  "name": "esy-pesy-starter",
  "version": "0.1.0",
  "description": "Reason workflow with Esy",
  "license": "MIT",
  "esy": {
    "build": "pesy",
    "install": "esy-installer",
    "buildsInSource": "_build"
  },
  "buildDirs": {
    "bin": {
      "name": "esy-pesy-starter.exe",
      "main": "Index",
      "flags": [
      ],
      "require": [
        "esy-peasy-starter.lib"
      ]
    },
    "lib": {
      "namespace": "EsyPesyStarter",
      "name": "esy-peasy-starter.lib",
      "require": [
        "bos"
      ]
    }
  },
  "dependencies": {
    "pesy": "*",
    "@esy-ocaml/reason": "^3.0.0",
    "refmterr": "^3.0.4",
    "@opam/bos": "*"
  },
  "peerDependencies": {
    "ocaml": "~4.6.0"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "^3.0.3",
    "ocaml": "~4.6.0"
  }
}
